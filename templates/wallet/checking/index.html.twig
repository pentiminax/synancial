{% extends 'base.html.twig' %}

{% block title %}Comptes bancaire{% endblock %}

{% block body %}
<div class="container mb-5">

    <div class="row">
        <h2>
            <a class="text-decoration-none text-dark" href="{{ path('wallet') }}">
                <i class="fas fa-long-arrow-alt-left"></i>
                Comptes bancaires
            </a>
        </h2>

        <div class="col-xl-4 offset-8 checking-accounts-list">
            <div class="d-flex justify-content-center">
                <div class="spinner-border" role="status">
                    <span class="visually-hidden">Loading...</span>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('checking') }}
{% endblock %}